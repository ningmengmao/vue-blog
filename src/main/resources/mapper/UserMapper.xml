<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ningmeng.vueblog.entity.User">
    <select id="getUser" resultType="com.ningmeng.vueblog.entity.User">

    </select>

    <select id="selectById" resultType="com.ningmeng.vueblog.entity.User">
        select id, username, user_url, avatar_url, account_id, create_time, bio
        from `b_user`
        where id = #{id}
    </select>
</mapper>